#include "main.h"
#include "stdlib.h"
#include "stdbool.h"
#include "gpio.h"
#include "TSAC_control.h"
#include "Slave_control.h"

void USB_control(uint8_t *usb_data)
{
	usb_data[12 * 2 + 2] = 0xff;
	usb_data[12 * 2] = 12 >> 8;
	usb_data[12 * 2 + 1] = 12;
	for(uint8_t i = 0; i < 12; i++)
	{
		usb_data[i] = 12[i];
		usb_data[12 + i] = 12[i];
	}

	CDC_Transmit_FS(usb_data, 12 * 2 + 2 + 1);
	//CDC_Transmit_FS(usb_data, 8);
}
